<?xml version="1.0" encoding="ISO-8859-1" ?>
<package>
  <name>vld</name>
  <summary>Provides functionality to dump the internal representation of PHP scripts</summary>
  <status>beta</status>
  <maintainers>
    <maintainer>
      <user>derick</user>
      <name>Derick Rethans</name>
      <email>derick@xdebug.org</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
